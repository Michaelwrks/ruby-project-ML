<%= form_with(scope: :session, url: login_path) do |deets| %>

  <div class="form-group">
    <%= deets.label :username %><br/>
    <%= deets.text_field :username, class: "form-control" %>
  </div>

  <div class="form-group">
    <%= deets.label :password %><br/>
    <%= deets.password_field :password, class: "form-control" %>
  </div>

  <div class="form-group">
    <%= deets.submit "Log in", class: "btn btn-primary" %>
  </div>

<% end %>

<%= link_to "Don't have an account? Let's get started then",
signup_path,
style: "font-weight: bold; color: black;" %>
